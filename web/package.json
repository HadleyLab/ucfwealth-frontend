{
    "name": "web",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "aidbox-react": "^1.3.3",
        "antd": "^4.16.13",
        "classnames": "^2.2.6",
        "codemirror": "^5.64.0",
        "final-form": "^4.20.1",
        "final-form-arrays": "^3.0.2",
        "js-yaml": "^4.1.0",
        "lodash": "^4.17.21",
        "react": "^17.0.2",
        "react-codemirror2": "^7.2.1",
        "react-dom": "^17.0.2",
        "react-final-form": "^6.5.2",
        "react-final-form-arrays": "^3.1.3",
        "react-router-dom": "^5.2.0",
        "react-scripts": "^5.0.1",
        "react-select": "^3.1.1",
        "shared": "0.0.1",
        "query-string": "~7.1.1",
        "buffer": "^6.0.3",
        "@craco/craco": "^6.4.3",
        "craco-babel-loader": "^1.0.3",
        "craco-less": "^2.0.0",
        "craco-antd": "^2.0.0"
    },
    "scripts": {
        "start": "craco start",
        "build": "craco build",
        "test": "craco test --watchAll=false",
        "eject": "react-scripts eject",
        "typecheck": "tsc"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jest": {
        "collectCoverageFrom": [
            "**/*.{ts,tsx}",
            "!**/node_modules/**",
            "!**/contrib/**"
        ],
        "coverageReporters": [
            "text-summary",
            "html"
        ],
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "transformIgnorePatterns": [
            "node_modules/(?!axios)$",
            "^.+\\.module\\.(css|sass|scss)$"
        ]
    },
    "devDependencies": {
        "@sentry/browser": "^5.27.1",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.3.2",
        "@testing-library/user-event": "^7.1.2",
        "@types/classnames": "^2.2.11",
        "@types/jest": "^26.0.15",
        "@types/js-yaml": "^4.0.5",
        "@types/node": "^12.0.0",
        "@types/react": "^17.0.34",
        "@types/react-dom": "^17.0.11",
        "@types/react-router-dom": "^5.1.6",
        "@types/react-select": "^3.0.26",
        "sass": "^1.51.0",
        "ts-jest": "^26.4.3",
        "typescript": "^4.4.4"
    },
    "eslintConfig": {
        "rules": {
            "no-unused-vars": 0,
            "@typescript-eslint/no-unused-vars": 0
        }
    }
}
